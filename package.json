{
  "name": "seleniumcucumber",
  "version": "1.6.2",
  "description": "JavaScript browser automation framework using official selenium-webdriver and cucumber-js",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/selenium-cucumber-js/index.js -s ./features/step-definitions",
    "output": "./reports/cucumber_report.html"
  },
  "author": {
    "name": "Soukaina Abid",
    "email": "abid.soukaina.90@gmail.com"
  },
  "license": "ISC",
  "keywords": [
    "selenium",
    "selenium bdd",
    "cucumber",
    "cucumber-js",
    "webdriver",
    "selenium-webdriver",
    "chromedriver",
    "testing",
    "junit",
    "bdd"
  ],
  "engineStrict": true,
  "dependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chromedriver": "^2.43.1",
    "commander": "^2.9.0",
    "cucumber": "^1.3.3",
    "cucumber-junit": "^1.6.0",
    "electron": "^1.8.8",
    "electron-chromedriver": "^1.8.0",
    "electron-packager": "^9.1.0",
    "electron-prebuilt": "^1.4.13",
    "eyes.selenium": "0.0.72",
    "fs-plus": "^2.9.1",
    "got": "^9.2.2",
    "merge": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "require-dir": "^0.3.2",
    "selenium-cucumber-js": "^1.6.2",
    "selenium-webdriver": "^3.5.0"
  },
  "devDependencies": {
    "cucumber-html-reporter": "^4.0.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.14.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": false,
      "browser": true
    },
    "globals": {
      "selenium": true,
      "helpers": true,
      "page": true,
      "driver": true,
      "until": true,
      "by": true,
      "expect": true,
      "Promise": true,
      "browserName": true,
      "DEFAULT_TIMEOUT": true
    }
  }
}
